{
  "name": "popcorn-time-experimental",
  "productName": "Popcorn Time",
  "version": "0.0.1",
  "description": "Experimental version of the popcorn time client",
  "main": "main.js",
  "scripts": {
    "build": "npm run styles && npm run build-main && npm run build-renderer",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "dev": "npm run styles && concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "hot-server": "node -r babel-register server.js",
    "lint": "eslint app test *.js",
    "styles": "gulp",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --timeout 10000 --inline-diffs --async-only --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --timeout 10000 --inline-diffs --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "test-all": "npm run build && npm run test && npm run test-e2e && npm run lint",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./main.development",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build --publish never --mac --win --linux",
    "precommit": "npm run test-all"
  },
  "wcjs-prebuilt": {
    "runtime": "electron",
    "runtimeVersion": "v1.2.6",
    "version": "v0.2.6"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/popcorn-desktop-experimental.git"
  },
  "author": {
    "name": "Amila Welihinda",
    "email": "amilajack@gmail.com",
    "url": "https://github.com/amilajack"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/popcorn-desktop-experimental/issues"
  },
  "keywords": [
    "popcorn-time",
    "popcorn",
    "time",
    "client",
    "electron",
    "react",
    "redux",
    "es6"
  ],
  "build": {
    "appId": "popcorn-time-experimental",
    "app-category-type": "public.app-category.video",
    "win": {
      "iconUrl": "https://cdn.rawgit.com/amilajack/popcorn-time-desktop/master/app/icons/app.ico"
    }
  },
  "homepage": "https://github.com/amilajack/popcorn-desktop-experimental#readme",
  "devDependencies": {
